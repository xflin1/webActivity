div.page.msg_success.js_show
    div.weui-msg
        div.weui-msg__icon-area
            i.weui-icon-warn.weui-icon_msg
        div.weui-msg__text-area
            h2.weui-msg__title 操作失败
            p.weui-msg__desc
        div.weui-msg__opr-area
            p.weui-btn-area
                a.weui-btn.weui-btn_primary.myaw(href="javascript:void(0)",ng-click="redirectTo('/serviceAction/add')") 继续添加
                a.weui-btn.weui-btn_default.myab(href="javascript:void(0)",ng-click="redirectTo('/serviceAction/list')") 返回列表
        div.weui-msg__extra-area
            div.weui-footer
                p.weui-footer__links
                p.weui-footer__text